
@{
    ViewData["Title"] = "CreteEvent";
}

<h2>CreteEvent</h2>
<form>
    <div class="form-group">
        <label for="exampleInputEmail1">Event Name</label>
        <input type="text" class="form-control" id="EventName" aria-describedby="emailHelp" placeholder="Enter Event Name">
        <small id="Info" class="form-text text-muted">Please enter a name for your Event </small>
    </div>
    <div class="form-group">
        <label for="DescriptionLabel">Event Description</label>
        <input type="text" class="form-control" id="EventDescription"  placeholder="Enter Event Name">
        
    </div>
    <div class="form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">Check me out</label>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
