
@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Theme Made By www.w3schools.com - No Copyright -->
    <title>Bootstrap Theme Company Page</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <style>
        .jumbotron {
            background-color: #f4511e;
            color: #fff;
            padding: 100px 25px;
        }

        .container-fluid {
            padding: 60px 50px;
        }

        .bg-grey {
            background-color: #f6f6f6;
        }

        .logo-small {
            color: #f4511e;
            font-size: 50px;
        }

        .logo {
            color: #f4511e;
            font-size: 200px;
        }
        }
    </style>
</head>
<body>


    <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
            <li data-target="#myCarousel" data-slide-to="3"></li>
        </ol>
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <img src="~/images/Melodifestivalen2018.svg" alt="ASP.NET" class="img-responsive" />
                <div class="carousel-caption" role="option">
                    <p>

                        <a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkID=525028&clcid=0x409">
                            More Infomation
                        </a>
                    </p>
                </div>
            </div>
            <div class="item">
                <img src="~/images/EdSheeran.svg" alt="Visual Studio" class="img-responsive" />
                <div class="carousel-caption" role="option">
                    <p>

                        <a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkID=525030&clcid=0x409">
                            More Infomation
                        </a>
                    </p>
                </div>
            </div>
            <div class="item">
                <img src="~/images/Eminem.svg" alt="Package Management" class="img-responsive" />
                <div class="carousel-caption" role="option">
                    <p>

                        <a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkID=525029&clcid=0x409">
                            More Infomation
                        </a>
                    </p>
                </div>
            </div>
            <div class="item">
                <img src="~/images/Summerburst.svg" alt="Microsoft Azure" class="img-responsive" />
                <div class="carousel-caption" role="option">
                    <p>

                        <a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkID=525027&clcid=0x409">
                            More Infomation
                        </a>
                    </p>
                </div>
            </div>
        </div>
        <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#firstTab">View All Events</a></li>
        <li><a data-toggle="tab" href="#secondTab">Search Events</a></li>
    </ul>
    <div class="tab-content">
        <div id="firstTab" class="tab-pane fade in active">tab 1 content</div>
        <div id="secondTab" class="tab-pane fade in">tab 2 content</div>
    </div>



    <div class="container-fluid">
        <div class="text-center">
            <h2>Våra event</h2>
        </div>

        <!-- Container (Products) -->
        <div class="row">
            <div class="col-sm-4 col-xs-12">
                <div class="panel panel-default text-center">
                    <div class="panel-heading">
                        <h1>Melodifestivalen</h1>
                    </div>
                    <div class="panel-body">
                        <p><strong>Platser:</strong>Sektion V99-V105</p>
                    </div>
                    <div class="panel-footer">
                        <h3>500:-</h3>
                        <h4>Oj vad billigt</h4>
                        <button class="btn btn-lg">KÖP</button>
                    </div>
                </div>
            </div>
            <div class="col-sm-4 col-xs-12">
                <div class="panel panel-default text-center">
                    <div class="panel-heading">
                        <h1>Ed Sheeran</h1>
                    </div>
                    <div class="panel-body">
                        <p><strong>Platser:</strong>Sektion Q13-Q15</p>
                    </div>
                    <div class="panel-footer">
                        <h3>250 :-</h3>
                        <h4>Oj vad billigt</h4>
                        <button class="btn btn-lg">KÖP</button>
                    </div>
                </div>
            </div>
            <div class="col-sm-4 col-xs-12">
                <div class="panel panel-default text-center">
                    <div class="panel-heading">
                        <h1>Eminem</h1>
                    </div>
                    <div class="panel-body">
                        <p><strong>Platser:</strong>Sektion S12-S14</p>
                    </div>
                    <div class="panel-footer">
                        <h3>700:-</h3>
                        <h4>Oj vad billigt</h4>
                        <button class="btn btn-lg">KÖP</button>
                    </div>
                </div>
            </div>
            <div class="col-sm-4 col-xs-12">
                <div class="panel panel-default text-center">
                    <div class="panel-heading">
                        <h1>Summerburst</h1>
                    </div>
                    <div class="panel-body">
                        <p><strong>Platser:</strong>Sektion stående</p>
                    </div>
                    <div class="panel-footer">
                        <h3>1200:-</h3>
                        <h4>Oj vad billigt</h4>
                        <button class="btn btn-lg">KÖP</button>
                    </div>
                </div>
            </div>

        </div>
    </div>


    <!-- Container (Services Section) -->
    <div class="container-fluid text-center">
        <h2>Tjänster</h2>
        <h4>Vad vi erbjuder</h4>
        <br>
        <div class="row">
            <div class="col-sm-4">
                <span class="glyphicon glyphicon-off logo-small"></span>
                <h4>Kraft</h4>
                <p>Kraftigt företag</p>
            </div>
            <div class="col-sm-4">
                <span class="glyphicon glyphicon-heart logo-small"></span>
                <h4>Kärlek</h4>
                <p>Mer kärlek åt folket</p>
            </div>
            <div class="col-sm-4">
                <span class="glyphicon glyphicon-lock logo-small"></span>
                <h4>Vi får jobbet gjort</h4>
                <p>Det kanske går långsamt men framsteg är alltid framsteg</p>
            </div>
        </div>
        <br><br>
        <div class="row">
            <div class="col-sm-4">
                <span class="glyphicon glyphicon-leaf logo-small"></span>
                <h4>Naturvänner</h4>
                <p>Vi tänker alltid på naturen i första hand</p>
            </div>
            <div class="col-sm-4">
                <span class="glyphicon glyphicon-certificate logo-small"></span>
                <h4>Certifierade</h4>
                <p>Våra operativsystem är defenitivt inte certiferade</p>
            </div>
            <div class="col-sm-4">
                <span class="glyphicon glyphicon-wrench logo-small"></span>
                <h4 style="color:#303030;">Hårt arbete</h4>
                <p>Vi jobbar som maskiner</p>
            </div>
        </div>
    </div>

</body>
</html>