@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/images/Melodifestivalen2018.svg" alt="ASP.NET" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>

                    <a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkID=525028&clcid=0x409">
                        More Infomation
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/EdSheeran.svg" alt="Visual Studio" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>

                    <a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkID=525030&clcid=0x409">
                        More Infomation
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/Eminem.svg" alt="Package Management" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>

                    <a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkID=525029&clcid=0x409">
                        More Infomation
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/Summerburst.svg" alt="Microsoft Azure" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>

                    <a class="btn btn-default" href="https://go.microsoft.com/fwlink/?LinkID=525027&clcid=0x409">
                        More Infomation
                    </a>
                </p>
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#firstTab">View All Events</a></li>
    <li><a data-toggle="tab" href="#secondTab">Search Events</a></li>
</ul>
<div class="tab-content">
    <div id="firstTab" class="tab-pane fade in active">tab 1 content</div>
    <div id="secondTab" class="tab-pane fade in">tab 2 content</div>
</div>
